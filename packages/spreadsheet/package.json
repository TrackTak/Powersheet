{
  "name": "@tracktak/powersheet",
  "version": "1.0.0",
  "description": "A Lightning Fast Spreadsheet for Businesses",
  "author": "TRACKTAK LTD",
  "source": "src/index.ts",
  "exports": "./dist/index.modern.js",
  "main": "./dist/index.js",
  "module": "./dist/index.module.js",
  "types": "dist/index.d.ts",
  "repository": "git://github.com/trackTak/powersheet.git",
  "license": "SEE LICENSE IN LICENSE.txt",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "spreadsheet",
    "excel",
    "table",
    "formulas",
    "grid",
    "sheets"
  ],
  "scripts": {
    "copy-icons": "copyfiles -f \"./src/spreadsheet/icons.svg\" ./dist/",
    "build:tsc:": "run-p build:tsc:*",
    "build:tsc:main": "tsc -p tsconfig.json",
    "build:tsc:module": "tsc -p tsconfig.module.json",
    "build": "microbundle src/index.ts -f modern,esm,cjs --no-compress",
    "watch:build": "microbundle watch src/index.ts -f modern,esm,cjs",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "run-s build:tsc test:*",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:unit": "nyc --silent ava",
    "check-cli": "run-s test diff-integration-tests check-integration-tests",
    "check-integration-tests": "run-s check-integration-test:*",
    "diff-integration-tests": "mkdir -p diff && rm -rf diff/test && cp -r test diff/test && rm -rf diff/test/test-*/.git && cd diff && git init --quiet && git add -A && git commit --quiet --no-verify --allow-empty -m 'WIP' && echo '\\n\\nCommitted most recent integration test output in the \"diff\" directory. Review the changes with \"cd diff && git diff HEAD\" or your preferred git diff viewer.'",
    "watch:test": "nyc --silent ava --watch",
    "cov": "run-s build:tsc test:unit cov:html cov:lcov && open-cli coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:lcov": "nyc report --reporter=lcov",
    "cov:send": "run-s cov:lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && open-cli dist/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out dist/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json dist/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d dist/docs",
    "prepublishOnly": "run-s build copy-icons",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-aws-s3 --bucket-path=storybook.powersheet.io/"
  },
  "engines": {
    "node": ">=10",
    "yarn": ">=1.19.2"
  },
  "peerDependenciesMeta": {
    "xlsx": {
      "optional": true
    }
  },
  "peerDependencies": {
    "hyperformula": "^1.2.0",
    "xlsx": "^0.17.2"
  },
  "optionalDependencies": {
    "a-color-picker": "^1.2.1"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "konva": "^8.2.2",
    "lodash": "^4.17.21",
    "material-components-web": "^13.0.0",
    "numfmt": "^2.1.0",
    "sass": "^1.36.0",
    "tiny-typed-emitter": "^2.1.0",
    "tippy.js": "^6.3.1",
    "undo-redo-manager": "^1.0.0"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@babel/core": "^7.14.8",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/html": "^6.3.6",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/storybook-deployer": "^2.8.10",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "ava": "^3.12.1",
    "babel-loader": "^8.2.2",
    "codecov": "^3.5.0",
    "copyfiles": "^2.4.1",
    "css-loader": "5.2.6",
    "currency-symbol-map": "^5.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "dayjs": "^1.10.7",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "gh-pages": "^3.1.0",
    "hammer-touchemulator": "hashchange/touchemulator#feature-detach",
    "hyperformula": "^1.2.0",
    "microbundle": "^0.13.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "open-cli": "^6.0.1",
    "prettier": "^2.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass-loader": "10",
    "style-loader": "2.0.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.0",
    "typescript": "^4.0.2",
    "xlsx": "^0.17.2"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "ava": {
    "failFast": true,
    "timeout": "60s",
    "typescript": {
      "rewritePaths": {
        "src/": "build/main/"
      }
    },
    "files": [
      "!build/module/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
